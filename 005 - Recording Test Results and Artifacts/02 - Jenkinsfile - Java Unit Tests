pipeline {
    agent { 
		docker { 
			image 'maven:latest' 
		}
	}
    stages {
        stage('tests') {
		steps {
			configFileProvider(
				[configFile(fileId: 'settings.xml',
					target_location: 'settings.xml')]) {
				sh 'mvn -s settings.xml clean install'
			}
		}
        }
    }
}
